{
  "name": "fratch-ui",
  "version": "1.8.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/JorgeRojo/fratch-ui.git"
  },
  "description": "React UI kit where each component can be used as a stand-alone module",
  "keywords": [
    "ui-components",
    "css-modules",
    "library",
    "react",
    "storybook",
    "typescript",
    "vite",
    "vitest"
  ],
  "author": "JorgeRojo <jorge.rojodiseno@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./components/index.js",
  "dependencies": {
    "lodash": "4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/react-vite": "7.5.3",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.46.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "simple-update-notifier": "2.0.0",
    "storybook": "7.5.3",
    "typescript": "5.2.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "format": "prettier --write ./*.{ts,json} ./.storybook/**/*.ts ./src/**/*.{ts,tsx,css}",
    "lint": "eslint --max-warnings=0 . --ext .ts,.tsx  --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false",
    "start": "storybook dev -p 6006",
    "build": "tsc && vite build --config vite.build.config.ts",
    "storybook:build": "storybook build",
    "storybook:preview": "yarn storybook:build && vite preview --outDir ./storybook-static/ --open /",
    "dist:publish": "yarn publish ./dist/ --non-interactive --disable-pnp --ignore-scripts",
    "dist": "yarn build && cp ./package.json ./dist/package.json && yarn pack --cwd ./dist/",
    "test": "vitest --run --config ./vitest.config.ts",
    "test:cover": "yarn test run --coverage",
    "prepare": "husky install"
  }
}
