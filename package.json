{
  "name": "fratch-ui",
  "version": "1.1.27",
  "repository": "git@github.com:JorgeRojo/fratch-ui.git",
  "author": "JorgeRojo <jorge.rojodiseno@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "components/index.js",
  "type": "module",
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.1.0",
    "@storybook/addon-interactions": "7.1.0",
    "@storybook/addon-links": "7.1.0",
    "@storybook/blocks": "7.1.0",
    "@storybook/builder-vite": "7.1.0",
    "@storybook/react": "7.1.0",
    "@storybook/react-vite": "7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "3.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jsdom": "^22.1.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "storybook": "7.1.0",
    "typescript": "^5.0.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "4.3.9",
    "vite-plugin-dts": "^3.1.0",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "format": "prettier --write ./*.{ts,json} ./.storybook/**/*.ts ./src/**/*.{ts,tsx}",
    "lint": "eslint ./*.{ts,json} ./.storybook/**/*.ts ./src/**/*.{ts,tsx} --fix",
    "start": "storybook dev -p 6006",
    "build": "ls ./ && rm -rf ./dist && tsc && cross-env NODE_OPTIONS='--max_old_space_size=8192' vite build --config vite.build.config.ts",
    "storybook:build": "storybook build",
    "storybook:preview": "yarn storybook:build && vite preview --outDir ./storybook-static/ --open /",
    "set-version": "yarn version",
    "copy-package-files": "ls ./ && cp ./package.json ./dist/ && cp ./LICENSE ./dist/ && cp ./README.md ./dist/",
    "dist": "yarn set-version && yarn copy-package-files && yarn pack --cwd ./dist/ && yarn publish-package",
    "test": "vitest --config ./vitest.config.ts",
    "test:cover": "vitest --config ./vitest.config.ts run --coverage",
    "publish-auth": "dotenv -- cross-var echo \"//registry.npmjs.org/:_authToken=%NODE_AUTH_TOKEN%\" > ./.npmrc",
    "publish-package": "yarn publish ./dist/ --non-interactive"
  }
}
